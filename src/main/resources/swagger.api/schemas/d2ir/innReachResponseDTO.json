{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "D2IR response",
  "type": "object",
  "discriminator": {
    "propertyName": "responseType"
  },
  "properties": {
    "status": {
      "description": "Status",
      "type": "string"
    },
    "reason": {
      "description": "Reason",
      "type": "string"
    },
    "errors": {
      "description": "Errors",
      "type": "array",
      "items": {
        "$ref": "innReachError.json"
      }
    },
    "responseType": {
      "description": "response dto type",
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "status",
    "reason",
    "errors",
    "responseType"
  ]
}

{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Library mapping",
  "type": "object",
  "properties": {
    "centralServerId": {
      "description": "Central server id",
      "type": "string",
      "format": "uuid"
    },
    "centralServerCode": {
      "description": "Central server code",
      "type": "string",
      "maxLength": 5
    },
    "agencies": {
      "description": "List of agencies for this central server",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "d2ir/agency.json"
      }
    }
  },
  "additionalProperties": false,
  "required": [
    "centralServerId",
    "centralServerCode",
    "agencies"
  ]
}
